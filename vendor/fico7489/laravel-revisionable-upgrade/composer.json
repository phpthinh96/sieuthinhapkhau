        "laravel revisionable upgrade",
        "laravel updated_by ",
        "laravel deleted_by",
        "laravel created_by",
        "laravel updated_at by key"
    ],
    "homepage": "https://github.com/fico7489/laravel-revisionable-upgrade",
    "support": {
        "issues": "https://github.com/fico7489/laravel-revisionable-upgrade/issues",
        "source": "https://github.com/fico7489/laravel-revisionable-upgrade"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Filip Horvat",
            "email": "filip.horvat@am2studio.hr",
            "homepage": "http://am2studio.hr",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/database": "5.*",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "Fico7489\\Laravel\\RevisionableUpgrade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fico7489\\Laravel\\RevisionableUpgrade\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
